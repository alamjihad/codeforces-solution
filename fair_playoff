#include<stdio.h>
int main()
{
	int t;
	scanf ("%d",&t);
	for (int i=0;i<t;++i)
	{
		int a,b,c,d,x,y;
		scanf ("%d %d %d %d",&a,&b,&c,&d);
		if(a>b)
		{
			x=a;
		}
		else
		{
			x=b;
		}
		if(c>d)
		{
			y=c;
		}
		else
		{
			y=d;
		}
		if(x+y>a+b&&x+y>c+d)
		{
			printf ("YES\n");
		}
		else
		{
			printf ("NO\n");
		}
	}
	return 0;
}
