#include<stdio.h>
int main()
{
    int n;
    int m;
    char str[5];
    scanf("%d",&m);
    for(int j=0;j<m;++j)
    {
        for(int i=0;i<4;++i)
        {
            scanf("%c",&str[i]);
        }
        if(str[1]=='a'&&str[2]=='b'&&str[3]=='c')
        {
            printf("YES\n");
        }
        else if(str[1]=='a')
        {
            printf("YES\n");
        }
        else if(str[2]=='b')
        {
            printf("YES\n");
        }
        else if(str[3]=='c')
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }
    return 0;
}
