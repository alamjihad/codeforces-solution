#include<stdio.h>
void ans()
{
		int n,arr[1000],sum=0;
		scanf ("%d",&n);
		for(int i=0;i<n;++i)
		{
			scanf ("%d",&arr[i]);
			sum=sum+arr[i];
		}
		if(sum==n)
		{
			printf ("0\n");
		}
		else if (sum<n)
		{
			printf ("1\n");
		}
		else
		{
			printf ("%d\n",sum-n);
		}
}
int main()
{
	int t;
	scanf ("%d",&t);
	for(int j=0;j<t;++j)
	{
	      ans();
	}
	return 0;
}
