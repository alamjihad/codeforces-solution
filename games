#include<stdio.h>
void ans(int n)
{
	int arr1[1000],arr2[1000],s=0,i,j;
	for(i=0;i<n;++i)
	{
		scanf ("%d %d",&arr1[i],&arr2[i]);
	}
	for(i=0;i<n;++i)
	{
		for(j=0;j<n;++j)
		{
			if(arr1[i]==arr2[j])
			{
				s++;
			}
		}
	}
	printf ("%d",s);
}
int main()
{
	int n;
	scanf ("%d",&n);
	ans(n);
	return 0;
}
