#include<stdio.h>
void ans()
{
	int n,a,b,c,d;
		scanf ("%d %d %d %d %d",&n,&a,&b,		&c,&d);
		int k,m;
		k=n*(a-b);
		m=n*(a+b);
		if(k>(c+d)||m<(c-d))
		{
			printf ("NO\n");
		}
		else
		{
			printf ("YES\n");
		}
}
int main()
{
	int t;
	scanf ("%d",&t);
	for(int i=0;i<t;++i)
	{
		ans();
	}
		return 0;
}
