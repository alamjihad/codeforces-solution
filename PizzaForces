#include<stdio.h>
void res(long long int n)
{
    	if(n<6)
    	{
    		printf ("15\n");
    	}
    	else
    	{
    		if(n%2!=0)
    		{
    			n+=1;
    			n=n*2.5;
    		}
    		else
    		{
    			n=n*2.5;
    		}
    		printf ("%lld\n",n);
    	}
}
int main()
{
    int t;
    scanf ("%d",&t);
    for(int i=0;i<t;++i)
    {
    	
	    long long int n;
    	scanf ("%lld",&n);
		res(n);
    }
   	return 0;
}
